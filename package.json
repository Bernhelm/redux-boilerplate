{
  "name": "boilerplate-redux",
  "version": "1.0.1",
  "description": "Redux Boilerplate with Webpack, Babel, SASS, mocha, istambul, express on board",
  "author": "Alberto Garc√≠a Mora",
  "license": "MIT",
  "homepage": "https://github.com/Bernhelm/redux-boilerplate.git",
  "scripts": {
    "prestart": "npm install",
    "start": "parallelshell 'npm run start:webpack' 'npm run start:watch' 'npm run start:express'",
    "start:webpack": "npm run dev",
    "start:watch": "parallelshell 'npm run watch:test'",
    "start:express": "parallelshell 'npm run express:build' 'npm run express:test'",
    "express:build": "node server/build.server.js",
    "express:test": "node server/test.server.js",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
    "build": "webpack -p --progress --mode production --config webpack.config.build.js",
    "test": "cross-env NODE_ENV=test nyc mocha --require test/setup.js app/**/*.spec.js",
    "watch:test": "watch 'npm run test' app/",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://cvgellhorn@github.com/cvgellhorn/webpack-boilerplate.git"
  },
  "dependencies": {
    "lodash": "4.17.10",
    "redux": "4.0.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-istanbul": "4.1.6",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.2.0",
    "css-loader": "0.28.11",
    "expect": "23.1.0",
    "express": "4.16.3",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "5.2.0",
    "node-sass": "4.9.0",
    "nyc": "12.0.2",
    "parallelshell": "3.0.2",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "watch": "1.0.2",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "4.1.3",
    "webpack-node-externals": "1.7.2"
  }
}
